{
    "name": "clone-publication-extension",
    "version": "1.0.0",
    "description": "Clone Publicatino",
    "author": "RWS",
    "homepage": "https://www.rws.com",
    "type": "module",
    "main": "../dist/clone-publication-extension/main.js",
    "files": [
        "../dist/clone-publication-extension/*"
    ],
    "scripts": {
        "build": "webpack --config ./webpack.prod.config.js --progress",
        "dev": "webpack serve --config ./webpack.dev.config.js --progress --env target=https://docs.tridiondemo.com manifest=../manifest.json config=../clone-publication.config",
        "pack": "docs-extensions pack --input=../ --output=../",
        "lint": "eslint . --fix && prettier --write \"src/**/*\""
    },
    "dependencies": {
        "@tridion-docs/extensions": "2.0.0",
        "antd": "^5.14.1",
        "axios": "^1.6.7",
        "react": "18.2.0",
        "react-dom": "18.2.0"
    },
    "devDependencies": {
        "@babel/core": "7.21.3",
        "@babel/plugin-transform-runtime": "7.21.0",
        "@babel/preset-env": "7.20.2",
        "@babel/preset-react": "7.18.6",
        "@babel/preset-typescript": "7.21.0",
        "@babel/runtime": "7.21.0",
        "@tridion-docs/extensions-cli": "1.0.4",
        "@types/react": "18.0.31",
        "@types/react-dom": "18.0.11",
        "@typescript-eslint/eslint-plugin": "5.57.0",
        "@typescript-eslint/parser": "5.57.0",
        "autoprefixer": "10.4.14",
        "babel-loader": "9.1.2",
        "babel-plugin-styled-components": "2.0.7",
        "css-loader": "6.7.3",
        "cssnano": "5.1.15",
        "eslint": "8.37.0",
        "eslint-config-prettier": "8.8.0",
        "eslint-formatter-pretty": "4.1.0",
        "eslint-plugin-react": "7.32.2",
        "eslint-plugin-react-hooks": "4.6.0",
        "fork-ts-checker-webpack-plugin": "7.3.0",
        "mini-css-extract-plugin": "2.7.5",
        "postcss": "8.4.21",
        "postcss-loader": "7.1.0",
        "prettier": "2.8.7",
        "typescript": "5.0.2",
        "webpack": "5.75.0",
        "webpack-cli": "5.0.1",
        "webpack-dev-server": "4.13.1"
    }
}
